  "scripts": {
    "start": "webpack-dev-server --progress --inline --hot --host 0.0.0.0 --port 3000",
    "build": "npm run clear &&  NODE_ENV=production webpack --bail",
    "clear": "rimraf www; mkdir www;",
    "init": "cp _env .env; npm i; cd api; ./yii db/create; ./yii migrate --interactive=0",
    "reinit": "api/yii db/recreate; api/yii migrate --interactive=0",
    "recreate": "npm run reinit; api/yii fill/init",
    "init-ci": "cd api; ./yii migrate --interactive=0",
    "mock": "cd api; ./yii fill/init",
    "mock-ci": "cd api; ./yii fill/init",
    "test-ci": "mocha --recursive -R xunit test/ > tests.xml",
    "test": "mocha --recursive test/",
    "dev": "set NODE_ENV=production && webpack-dev-server --progress --hot --host 0.0.0.0 --port 3000",
    "prd": "set NODE_ENV=production && webpack --progress --inline"
